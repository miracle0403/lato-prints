<!-- Portfolio Grid -->
  <section class="bg-light" id="portfolio">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <h2 class="section-heading text-uppercase">Our Products</h2>
          <h3 class="section-subheading text-muted">These are some of the jobs we do.</h3>
        </div>
      </div>
	  
	  <div class="row">
		{{#if videos}}
			{{#each videos}}
			
			<div class="col-md-4 col-sm-6 portfolio-item">
          <a class="portfolio-link" data-toggle="modal" href="#portfolioModal5">
            <div class="portfolio-hover">
              <div class="portfolio-hover-content">
                <i class="fas fa-plus fa-3x"></i>
              </div>
            </div>
            <vid class="img-fluid" src="{{link}}" alt="{{title}} made by latolak prints">
          </a>
          <div class="portfolio-caption">
            <h4>{{title}}</h4><!--
           
            <div class = "p text-left text-muted">
            		{{description}}
            </div>
          </div>
        </div>
        
			{{/each}}
		{{/if}}
	  </div>
	  
	  
	</div>
  </section>